    {
      "parameters": {
        "jsCode": "// Get lead data from input\nconst leadData = $input.first().json;\n\n// Calculate lead priority based on score\nlet priority = 'Low';\nlet urgency = 'Normal';\nlet recommendedActions = [];\n\nif (leadData.score >= 80) {\n  priority = 'High';\n  urgency = 'Immediate';\n  recommendedActions = [\n    'Call within 1 hour',\n    'Send personalized proposal',\n    'Schedule demo meeting',\n    'Assign to senior sales rep'\n  ];\n} else if (leadData.score >= 50) {\n  priority = 'Medium';\n  urgency = 'Within 24 hours';\n  recommendedActions = [\n    'Send follow-up email',\n    'Add to nurture campaign',\n    'Schedule follow-up call'\n  ];\n} else {\n  priority = 'Low';\n  urgency = 'Within 1 week';\n  recommendedActions = [\n    'Add to general newsletter',\n    'Monitor for engagement'\n  ];\n}\n\n// Generate lead insights\nconst insights = {\n  leadScore: leadData.score,\n  priority: priority,\n  urgency: urgency,\n  recommendedActions: recommendedActions,\n  companySize: leadData.companySize || 'Unknown',\n  industry: leadData.industry || 'Unknown',\n  budget: leadData.budget || 'Not specified',\n  timeline: leadData.timeline || 'Not specified',\n  source: leadData.source || 'Unknown',\n  lastActivity: leadData.lastActivity || new Date().toISOString(),\n  processedAt: new Date().toISOString()\n};\n\n// Return original lead data plus insights\nreturn {\n  ...leadData,\n  ...insights\n};"
      },
